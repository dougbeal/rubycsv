<%# "Date","Time","TimeZone","Name","Type","Status","Currency","Gross","Fee","Net","From Email Address","To Email Address","Transaction ID","Shipping Address","Address Status","Item Title","Item ID","Shipping and Handling Amount","Insurance Amount","Sales Tax","Option 1 Name","Option 1 Value","Option 2 Name","Option 2 Value","Reference Txn ID","Invoice Number","Custom Number","Quantity","Receipt ID","Balance","Address Line 1","Address Line 2/District/Neighborhood","Town/City","State/Province/Region/County/Territory/Prefecture/Republic","Zip/Postal Code","Country","Contact Phone Number","Subject","Note","Country Code","Balance Impact" -%>
<%# Date and transaction -%>
<% @date_header = csvrow.first[0] -%>
<% @date_format = "%m/%d/%Y" -%>
<% @item = ['Type', 'Item Title', 'Name'] -%>
<% @cust = ['Name', 'Customer ID'] -%>
<% @type = csvrow['Type'] %>
<%= clean_date(csvrow[@date_header], @date_format)
%> <%= @item.map{ |c| csvrow[c] }.select{ |c| not c.strip.empty? }.join(', ') %> by <%= @cust.map{ |c| csvrow[c] }.join(', ') %> as <%= csvrow['Transaction ID']  %>
<%# -%>
<%# tags -%>
<% {'syncid' => 'Transaction ID',
}.each do |name, column|
%><% if csvrow[column]
  %><%= @ledger_tag % [ name, csvrow[column] ] %>
<% end -%><% end -%>
<%# Withdrawal -%>
<%#  -%>
<% if @type.eql?("General Withdrawal")
-%><% for @income in ['Net']
   -%><%= @ledger_transaction % ["Assets:Paypal", "$" + clean_money(csvrow[@income])] %>
<% end -%>
<%= @ledger_transaction % ["Transfer:Paypal", ""] %>
<%# Income -%>
<%#  -%>
<% else
-%><% for @income in ['Gross', 'Fee', 'Net']
   -%><% @money = clean_money(csvrow[@income])
      -%><% if not @money.empty? -%><%= @ledger_transaction % ["Assets:Paypal:" + @income + "", "$" + @money ] %>
      <%- end -%>
<% end -%>
<%# -%>
<%= @ledger_transaction % ["Income:Paypal", ""] %>
<% end -%>
